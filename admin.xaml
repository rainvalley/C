<Window x:Class="宿舍管理系统_WPF.admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:宿舍管理系统_WPF"
        mc:Ignorable="d"
        Title="管理员页面" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Label Content="选择您需要管理的信息类型：" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="371" FontSize="20px"/>
        <ComboBox Name="typebox" HorizontalAlignment="Left" Margin="24,77,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="typeboxchanged"/>
        <Label Content="选择您需要管理的具体信息：" HorizontalAlignment="Left" Margin="10,175,0,0" VerticalAlignment="Top" Width="301" FontSize="20px"/>
        <ComboBox Name="infobox" HorizontalAlignment="Left" Margin="24,288,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="infoboxchanged"/>
        <TextBlock FontSize="15px" HorizontalAlignment="Left" Margin="497,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" Height="21"><Run Text="用户信息输入："/><LineBreak/><Run/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="387,81,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top" Height="17"/>
        <TextBlock HorizontalAlignment="Left" Margin="538,79,0,0" TextWrapping="Wrap" Text="密码" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <Button Name="user_submit" Content="提交" HorizontalAlignment="Left" Margin="709,79,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.96,0.734" Click="user_submit_Click"/>
        <TextBox Name="ID" HorizontalAlignment="Left" Height="18" Margin="405,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox Name="passwd" HorizontalAlignment="Left" Height="18" Margin="567,80,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="passwd_TextChanged"/>
        <TextBlock FontSize="15px" HorizontalAlignment="Left" Margin="497,143,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" Height="21"><Run Text="学生"/><Run Text="信息输入："/><LineBreak/><Run/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="422,175,0,0" TextWrapping="Wrap" Text="学号" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="599,175,0,0" TextWrapping="Wrap" Text="姓名" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="386,297,0,0" TextWrapping="Wrap" Text="寝室长姓名" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="410,255,0,0" TextWrapping="Wrap" Text="床位号" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="398,213,0,0" TextWrapping="Wrap" Text="电话号码" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="599,297,0,0" TextWrapping="Wrap" Text="寝室长电话" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="599,255,0,0" TextWrapping="Wrap" Text="寝室长学号" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBlock HorizontalAlignment="Left" Margin="599,213,0,0" TextWrapping="Wrap" Text="宿舍号" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"/>
        <TextBox Name="id" HorizontalAlignment="Left" Height="23" Margin="461,175,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="649,171,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="649,171,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="name" HorizontalAlignment="Left" Height="23" Margin="649,171,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="phone" HorizontalAlignment="Left" Height="23" Margin="461,212,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="dorm_id" HorizontalAlignment="Left" Height="23" Margin="649,212,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="bed_id" HorizontalAlignment="Left" Height="23" Margin="461,251,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="head_id" HorizontalAlignment="Left" Height="23" Margin="664,251,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="head_name" HorizontalAlignment="Left" Height="23" Margin="461,296,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="head_phone" HorizontalAlignment="Left" Height="23" Margin="664,293,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.497,0.545"/>
        <TextBox Name="year"  HorizontalAlignment="Left" Height="23" Margin="461,340,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Name="stu_submit" Content="提交" HorizontalAlignment="Left" Margin="678,351,0,0" VerticalAlignment="Top" Width="75" Click="stu_submit_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="386,340,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="-0.175,0.604" Height="19"><Run Text="毕业生年份"/><LineBreak/><Run/></TextBlock>

    </Grid>
</Window>
